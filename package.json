{
  "name": "@watchmen/mongo-batch",
  "version": "1.2.0",
  "description": "mongo batch stuff",
  "main": "dist/index.js",
  "repository": "the-watchmen/node-mongo-batch",
  "license": "ISC",
  "scripts": {
    "ava": "cross-env NODE_ENV=auto ava -s test/ava",
    "ava-debug": "cross-env DEBUG=test:*,lib:* NODE_ENV=auto ava -s",
    "pretest": "xo",
    "test": "yarn ava && yarn cuke",
    "prebuild": "yarn test",
    "build": "rm -rf dist && babel src -d dist",
    "cuke": "cross-env DEBUG=test:*,app:*,lib:*,dbg:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register test/cuke",
    "cuke-feature": "cross-env DEBUG=test:*,app:*,lib:*,dbg:* NODE_ENV=auto cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register"
  },
  "dependencies": {
    "@watchmen/debug": "^1.0.2",
    "@watchmen/geocodr": "^1.1.9",
    "@watchmen/helpr": "^2.2.2",
    "@watchmen/mongo-data": "^2.0.2",
    "@watchmen/mongo-helpr": "^1.1.0",
    "@watchmen/tymer": "^1.2.4",
    "babel-polyfill": "^6.23.0",
    "config": "^1.29.0",
    "debug": "^3.1.0",
    "joi": "^13.0.2",
    "lodash": "^4.17.4",
    "mongodb": "^3.0.1",
    "require-uncached": "^1.0.3"
  },
  "devDependencies": {
    "@watchmen/cuke-helpr": "^3.1.6",
    "@watchmen/mongo-cuke-helpr": "^0.3.7",
    "@watchmen/mongo-test-helpr": "^1.0.0",
    "@watchmen/test-helpr": "^0.3.4",
    "ava": "^0.24.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.1.2",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^5.1.3",
    "cucumber": "^3.2.0",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "iso-639-1": "^2.0.3",
    "prettier": "^1.9.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "useEslintrc": true
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
